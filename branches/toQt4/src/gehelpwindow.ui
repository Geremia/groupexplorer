            <string>&amp;Increase font size</string>
        </property>
        <property name="accel">
            <string>Ctrl+Up</string>
        </property>
    </action>
    <action>
        <property name="name">
            <cstring>FontDecreaseAction</cstring>
        </property>
        <property name="iconSet">
            <iconset>decrease_font.png</iconset>
        </property>
        <property name="text">
            <string>Decrease font size</string>
        </property>
        <property name="menuText">
            <string>&amp;Decrease font size</string>
        </property>
        <property name="accel">
            <string>Ctrl+Down</string>
        </property>
    </action>
    <action>
        <property name="name">
            <cstring>CopyAction</cstring>
        </property>
