    <action>
        <property name="name">
            <cstring>helpSearchAction</cstring>
        </property>
        <property name="iconSet">
            <iconset>searchdocs.png</iconset>
        </property>
        <property name="text">
            <string>Search help...</string>
        </property>
        <property name="menuText">
            <string>&amp;Search help...</string>
        </property>
    </action>
</actions>
<connections>
    <connection>
        <sender>FindEdit</sender>
        <signal>returnPressed()</signal>
        <receiver>FindButton</receiver>
        <slot>animateClick()</slot>
    </connection>
    <connection>
        <sender>FindAction</sender>
        <signal>activated()</signal>
        <receiver>GEHelpWindow</receiver>
        <slot>FindAction_activated()</slot>
    </connection>
    <connection>
        <sender>FileCloseAction</sender>
        <signal>activated()</signal>
        <receiver>GEHelpWindow</receiver>
        <slot>FileCloseAction_activated()</slot>
    </connection>
    <connection>
        <sender>PrintAction</sender>
        <signal>activated()</signal>
        <receiver>GEHelpWindow</receiver>
        <slot>PrintAction_activated()</slot>
    </connection>
    <connection>
        <sender>SaveAsHTMLAction</sender>
        <signal>activated()</signal>
        <receiver>GEHelpWindow</receiver>
        <slot>SaveAsHTMLAction_activated()</slot>
    </connection>
    <connection>
        <sender>FindButton</sender>
        <signal>clicked()</signal>
        <receiver>GEHelpWindow</receiver>
        <slot>FindButton_clicked()</slot>
    </connection>
</connections>
<includes>
    <include location="global" impldecl="in declaration">qstringlist.h</include>
    <include location="local" impldecl="in declaration">gehelpbrowser.h</include>
    <include location="global" impldecl="in declaration">myutils.h</include>
    <include location="global" impldecl="in declaration">qtundo.h</include>
    <include location="global" impldecl="in implementation">qstatusbar.h</include>
    <include location="local" impldecl="in implementation">gewhatsthis.h</include>
    <include location="global" impldecl="in implementation">printing.h</include>
    <include location="global" impldecl="in implementation">qmessagebox.h</include>
    <include location="global" impldecl="in implementation">qfile.h</include>
    <include location="global" impldecl="in implementation">qfiledialog.h</include>
    <include location="local" impldecl="in implementation">mainwindow.h</include>
    <include location="local" impldecl="in implementation">gehelpwindow.ui.h</include>
</includes>
<variables>
    <variable access="private">static GEHelpWindow* mainHelpWindow;</variable>
    <variable access="private">GEHelpBrowser* browser;</variable>
    <variable access="private">QtUndoStack* undoStack;</variable>
</variables>
<signals>
    <signal>actionRequested( QStringList parameters, QWidget* originator )</signal>
    <signal>newHelpWindowCreated( GEHelpWindow* newwin );</signal>
    <signal>gotResized()</signal>
    <signal>gotMoved()</signal>
</signals>
<slots>
    <slot specifier="static">showHelp( QString doc )</slot>
    <slot>goTo( QString ref )</slot>
    <slot>showTo( QString ref )</slot>
    <slot>showAndFocus()</slot>
    <slot>browserRequestedAction( QStringList parameters )</slot>
    <slot>setText( QString doc )</slot>
    <slot>scrollToAnchor( QString anchor )</slot>
    <slot>SaveAsHTMLAction_activated()</slot>
    <slot>PrintAction_activated()</slot>
    <slot>FileCloseAction_activated()</slot>
    <slot>FindAction_activated()</slot>
    <slot>FindButton_clicked()</slot>
</slots>
<functions>
    <function access="private" specifier="non virtual">init()</function>
    <function specifier="static" returnType="GEHelpWindow *">getMainHelpWindow()</function>
    <function access="private">notifyAboutNewWindow( GEHelpWindow * newwin )</function>
    <function>pushCommand( QtCommand * cmd )</function>
    <function access="private">resizeEvent( QResizeEvent * e )</function>
    <function access="private">moveEvent( QMoveEvent * e )</function>
    <function returnType="QString">title()</function>
</functions>
