<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>GEHelpWindow</class>
<widget class="QMainWindow">
    <property name="name">
        <cstring>GEHelpWindow</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>719</width>
            <height>771</height>
        </rect>
    </property>
    <property name="caption">
        <string>Group Explorer Help</string>
    </property>
    <property name="icon">
        <pixmap>helpdocs.png</pixmap>
    </property>
</widget>
<menubar>
    <property name="name">
        <cstring>HelpMenu</cstring>
    </property>
    <item text="&amp;File" name="File">
        <action name="SaveAsHTMLAction"/>
        <action name="PrintAction"/>
        <separator/>
        <action name="FileCloseAction"/>
    </item>
    <item text="&amp;Go" name="Go">
        <action name="HomeAction"/>
        <separator/>
        <action name="BackAction"/>
        <action name="ForwardAction"/>
    </item>
    <item text="&amp;Edit" name="Edit">
        <action name="FontIncreaseAction"/>
        <action name="FontDecreaseAction"/>
        <separator/>
        <action name="CopyAction"/>
        <separator/>
        <action name="FindAction"/>
        <action name="helpSearchAction"/>
    </item>
</menubar>
<toolbars>
    <toolbar dock="2">
        <property name="name">
            <cstring>Toolbar</cstring>
        </property>
        <property name="label">
            <string>Toolbar</string>
        </property>
        <action name="HomeAction"/>
        <separator/>
        <action name="BackAction"/>
        <action name="ForwardAction"/>
        <separator/>
        <action name="FontIncreaseAction"/>
        <action name="FontDecreaseAction"/>
        <separator/>
        <action name="CopyAction"/>
        <separator/>
        <action name="SaveAsHTMLAction"/>
        <action name="PrintAction"/>
        <separator/>
        <widget class="QLineEdit">
            <property name="name">
                <cstring>FindEdit</cstring>
            </property>
        </widget>
        <widget class="QPushButton">
            <property name="name">
                <cstring>FindButton</cstring>
            </property>
            <property name="text">
                <string>Find</string>
            </property>
        </widget>
        <action name="helpSearchAction"/>
    </toolbar>
</toolbars>
<actions>
    <action>
        <property name="name">
            <cstring>HomeAction</cstring>
        </property>
        <property name="iconSet">
            <iconset>home.png</iconset>
        </property>
        <property name="text">
            <string>Home</string>
        </property>
        <property name="menuText">
            <string>&amp;Home</string>
        </property>
        <property name="accel">
            <string>Ctrl+Home</string>
        </property>
    </action>
    <action>
        <property name="name">
            <cstring>BackAction</cstring>
        </property>
        <property name="iconSet">
            <iconset>previous.png</iconset>
        </property>
        <property name="text">
            <string>Back</string>
        </property>
        <property name="menuText">
            <string>&amp;Back</string>
        </property>
        <property name="accel">
            <string>Ctrl+Left</string>
        </property>
    </action>
    <action>
        <property name="name">
            <cstring>ForwardAction</cstring>
        </property>
        <property name="iconSet">
            <iconset>next.png</iconset>
        </property>
        <property name="text">
            <string>Forward</string>
        </property>
        <property name="menuText">
            <string>&amp;Forward</string>
        </property>
        <property name="accel">
            <string>Ctrl+Right</string>
        </property>
    </action>
    <action>
        <property name="name">
            <cstring>FontIncreaseAction</cstring>
        </property>
